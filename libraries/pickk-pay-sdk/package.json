{
  "name": "@pickk/pay",
  "version": "1.1.4",
  "description": "PICKK Payment module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEV-MUGLES/pickk-payments.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "payments",
    "inicis",
    "kg"
  ],
  "author": "greatSumini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DEV-MUGLES/pickk-payments/issues"
  },
  "homepage": "https://github.com/DEV-MUGLES/pickk-payments#readme",
  "scripts": {
    "build": "rimraf -rf dist && babel lib --out-dir dist --extensions '.ts' && tsc -p ./tsconfig.json",
    "format": "prettier --write **/*.{ts,json} --ignore-path ./.prettierignore",
    "lint": "eslint \"lib/**/*.ts\" --fix",
    "type-check": "tsc --noEmit --emitDeclarationOnly false",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prepublish:npm": "npm run format && npm run lint && npm run type-check && npm run test && npm run build",
    "publish:npm": "npm publish --access public",
    "prerelease": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "peerDependencies": {
    "axios": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@types/faker": "^5.5.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "axios": "^0.21.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.3.5",
    "webpack-cli": "^4.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage"
  },
  "dependencies": {
    "inicis": "^1.1.2"
  }
}
